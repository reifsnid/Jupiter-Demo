            <tgroup cols="2">
                <colspec colname="c1" colnum="1" colwidth="1*"/>
                <colspec colname="c2" colnum="2" colwidth="3*"/>
                <thead>
                    <row>
                        <entry>Submodule</entry>
                        <entry>Description</entry>
                    </row>
                </thead>
                <tbody>
                    <row>
                        <entry>Io</entry>
                        <entry><ph id="Io-description-ph">Provides dashboard reports on the
